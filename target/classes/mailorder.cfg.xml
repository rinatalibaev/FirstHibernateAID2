<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="first.MailOrder" table="MailOrder">
		<id name="mailOrdNo" column="MailOrdNo">
			<generator class="native" />
		</id>	
		<property name="mailOrdToSendDate" column="MailOrdToSendDate" type="date"/>
		<property name="mailOrdSentDate" column="MailOrdSentDate" type="timestamp"/>
		<property name="mailOrdStatus" column="MailOrdStatus" type="string"/>
		<property name="mailOrdCreateDate" column="MailOrdCreateDate" type="timestamp"/>
		<many-to-one name="mailOrdReceiverNo" class="first.Employee">
			<column name="MailOrdReceiverNo" not-null="true"/>
		</many-to-one>
		<many-to-one name="mailOrdSenderNo" class="first.Employee">
			<column name="MailOrdSenderNo" not-null="true"/>
		</many-to-one>
	</class>
</hibernate-mapping>