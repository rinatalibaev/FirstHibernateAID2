<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.paint.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DBEmployeeWindowController">
   <children>
      <AnchorPane prefHeight="105.0" prefWidth="1080.0">
         <children>
            <TableView fx:id="DBEmpTable" prefHeight="230.0" prefWidth="1080.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
              <columns>
              		  <TableColumn fx:id="DBEmpTableId" prefWidth="75.0" text="ID" />
                      <TableColumn fx:id="DBEmpTableColumn1" prefWidth="75.0" text="Фамилия" />
                      <TableColumn fx:id="DBEmpTableColumn2" prefWidth="75.0" text="Имя" />
                      <TableColumn fx:id="DBEmpTableColumn3" prefWidth="75.0" text="Отчество" />
                      <TableColumn fx:id="DBEmpTableColumn4" prefWidth="75.0" text="Должность" />
                      <TableColumn fx:id="DBEmpTableColumn5" prefWidth="75.0" text="Эл.почта" />
                      <TableColumn fx:id="DBEmpTableColumn6" prefWidth="75.0" text="Телефон" />
                      <TableColumn fx:id="DBEmpTableColumn7" prefWidth="75.0" text="Доб.телеф." />
                      <TableColumn fx:id="DBEmpTableColumn8" prefWidth="75.0" text="Регион" />
                      <TableColumn fx:id="DBEmpTableColumn9" prefWidth="75.0" text="Город" />
                      <TableColumn fx:id="DBEmpTableColumn10" prefWidth="75.0" text="Улица" />
                      <TableColumn fx:id="DBEmpTableColumn11" prefWidth="75.0" text="Дом" />
                      <TableColumn fx:id="DBEmpTableColumn12" prefWidth="75.0" text="Корп. дома" />
                      <TableColumn fx:id="DBEmpTableColumn13" prefWidth="75.0" text="Офис" />
                    </columns>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
            </TableView>
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </AnchorPane>
      <AnchorPane prefHeight="45.0" prefWidth="1100.0">
         <children>
            <HBox prefHeight="45.0" prefWidth="1100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#DBEmployeeAdding" text="Добавить">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onKeyPressed="#DBEmployeeEditing" onMouseClicked="#DBEmployeeEditing" text="Изменить">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#DBEmployeeDeleting" text="Удалить">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#closeDBEmployeeWindow" prefHeight="25.0" prefWidth="58.0" text="ОК" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="10.0" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#closeDBEmployeeWindow" text="Отмена" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </AnchorPane>
   </children>
</VBox>
